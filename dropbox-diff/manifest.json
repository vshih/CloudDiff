{
	"name": "DropboxDiff",
	"short_name": "DropboxDiff",
	"description": "Compare versions of your Dropbox files in-browser or with your configured diff tool. Trigger from any \"Version history\" page.",
	"version": "1.0.2.16",

	"manifest_version": 2,
	"background": {
		"page": "background.html",
		"persistent": false
	},
	"content_scripts": [
		{
			"css": [
				"diffview.css",
				"jquery.alertable.css",
				"content.css"
			],
			"js": [
				"jquery-1.8.0.min.js",
				"jquery.alertable.min.js",
				"difflib.js",
				"diffview.js",
				"ex-handler.js",
				"content.js"
			],
			"matches": [ "https://www.dropbox.com/history/*" ],
			"run_at": "document_end"
		}
	],
	"web_accessible_resources": [
		"icon_spacer-vflN3BYt2.gif",
		"content-inject.js"
	],
	"content_security_policy": "default-src 'self' https://*.dropbox.com",
	"icons": {
		"128": "128.png",
		"16": "16.png",
		"48": "48.png"
	},
	"minimum_chrome_version": "22.0.0.0",
	"options_page": "options.html",
	"permissions": [
		"https://*.dropbox.com/*",
		"nativeMessaging"
	]
}
