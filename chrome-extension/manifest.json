{
	"manifest_version": 2,
	"name": "CloudDiff",
	"version": "1.0.2.24",

	"description": "Compare versions of your cloud files in-browser or using your configured diff tool.",
	"icons": {
		"16": "16.png",
		"48": "48.png",
		"128": "128.png"
	},

	"author": "Victor Shih <victor.shih@gmail.com>",
	"background": {
		"page": "background.html",
		"persistent": false
	},
	"content_scripts": [
		{
			"css": [
				"jquery.alertable.css",
				"diffview.css",
				"content.css"
			],
			"js": [
				"jquery-1.8.0.min.js",
				"jquery.alertable.min.js",
				"difflib.js",
				"diffview.js",
				"ex-handler.js",
				"content.js"
			],
			"matches": [
				"https://www.dropbox.com/history/*"
			],
			"run_at": "document_end"
		}
	],
	"content_security_policy": "default-src 'self' https://*.dropbox.com",
	"minimum_chrome_version": "22.0.0.0",
	"offline_enabled": false,
	"options_page": "options.html",
	"permissions": [
		"https://*.dropbox.com/*",
		"nativeMessaging"
	],
	"short_name": "CloudDiff",
	"web_accessible_resources": [
		"content-inject.js",
		"icon_spacer-vflN3BYt2.gif"
	]
}
